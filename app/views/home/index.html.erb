<table>
<h2> <%= @student.student_genius %>  </h2>
  <thead>
    <tr>
      <th> Course </th>
      <th> Professor </th>
      <th> Grade </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student.registrations.each do |registration| %>
      <tr>
        <td><%= registration.course %></td>
        <td><%= registration.course.professor.prof_name %></td>
        <td><%= registration.grade_format %></td>
      </tr>
    <% end %>
  </tbody>
</table>
